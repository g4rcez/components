{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "cursorOffset": -1,
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": false,
  "printWidth": 150,
  "quoteProps": "as-needed",
  "rangeEnd": 0,
  "rangeStart": 0,
  "semi": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
